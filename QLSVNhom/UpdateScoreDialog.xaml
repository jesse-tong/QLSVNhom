<Window x:Class="QLSVNhom.UpdateScoreDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Update Score" Height="250" Width="300">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="MAHP:" />
            <TextBox x:Name="txtMAHP" Margin="0,0,0,10" />
            <TextBlock Text="DIEMSO:" />
            <TextBox x:Name="txtDIEMSO" Margin="0,0,0,10" />
            <Button Content="Update" Click="Update_Click" HorizontalAlignment="Right" />
        </StackPanel>
    </Grid>
</Window>
