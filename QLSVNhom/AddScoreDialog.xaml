<Window x:Class="QLSVNhom.AddScoreDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add New Score" Height="250" Width="300">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="MAHP:" />
            <TextBox x:Name="txtMAHP" Margin="0,0,0,10" />
            <TextBlock Text="MALOP:" />
            <TextBox x:Name="txtMALOP" Margin="0,0,0,10" />
            <TextBlock Text="MASV:" />
            <TextBox x:Name="txtMASV" Margin="0,0,0,10" />
            <TextBlock Text="DIEMSO:" />
            <TextBox x:Name="txtDIEMSO" Margin="0,0,0,10" />
            <Button Content="Add" Click="Add_Click" HorizontalAlignment="Right" />
        </StackPanel>
    </Grid>
</Window>
